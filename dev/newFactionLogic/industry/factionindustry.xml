      <params>
        <param name="FactionManagerCue" />
        <param name="Faction" />
        <param name="DebugChance" default="0" />
      </params>
      <cues>
        <cue name="Manage_Stations" ref="md.FactionIndustry_Stations.Manage_Stations">
          <param name="FactionManagerCue" value="$FactionManagerCue" />
