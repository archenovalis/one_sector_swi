<?xml version="1.0" encoding="utf-8"?>
<mdscript name="FactionIndustry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../xsd/md.xsd">
  <cues>
    <library name="Industry_Manager" namespace="this">
      <params>
        <param name="FactionManagerCue" />
        <param name="Faction" />
        <param name="DebugChance" default="0" />
      </params>
      <cues>
        <cue name="Build_Stations" ref="md.FactionIndustry_Stations.Build_Stations">
          <param name="FactionManagerCue" value="$FactionManagerCue" />
          <param name="Faction" value="$Faction" />
          <param name="DebugChance" value="$DebugChance" />
        </cue>
        <cue name="Build_Ships" ref="md.FactionIndustry_Ships.Build_Ships">
          <param name="FactionManagerCue" value="$FactionManagerCue" />
          <param name="Faction" value="$Faction" />
          <param name="DebugChance" value="$DebugChance" />
        </cue>
      </cues>
    </library>
  </cues>
</mdscript>